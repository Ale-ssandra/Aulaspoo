
public class ExemploVariaveis {

	public static void main(String[] args) {
		String nome = new String ("Ana");
		final int idade =20;
		float altura = 1.89f;
		double peso = 60.00;
		System.out.println("Dados\n"   +idade +  "\n" +altura+ "\n"+ peso);
		System.out.println(nome.compareToIgnoreCase(nome));
	}

}
